#include<iostream>
#include<conio.h>
using namespace std;
class Student
{
public:
	Student(char *name1,int idd=0);
	~Student();
	void Add();
	void Cut();
	void Amend();
	void Find();
	void Display();
private:
	char *name;
	int id;
	int grade;
};
Student::Student(char *name1,int idd)
{
	name=new char[strlen(name1)+1];
	strcpy(name,name1);
	id=idd;
}
void Student::Add()
{
	cin>>name>>id>>grade;
}



